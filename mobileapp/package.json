{
  "name": "mobileapp",
  "main": "expo-router/entry",
  "expo": {
    "app": {
      "extends": "./app.config.ts"
    },
    "install": {
      "exclude": [
        "expo-updates"
      ]
    }
  },
  "version": "1.0.0",
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md,yml,yaml}\"",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "expo lint",
    "typecheck": "tsc -b",
    "build:android:placeholder": "npx -y eas-cli build --profile placeholder --platform android --local --non-interactive --output ./app-placeholder.apk",
    "prebuild:ios:placeholder": "APP_ID=placeholder npx expo prebuild --platform ios",
    "buildui:ios:placeholder": "open ios/Placeholder.xcworkspace",
    "build:android:champs-tracker": "npx -y eas-cli build --profile champs-tracker --platform android --local --non-interactive --output ./app-champs-tracker.apk",
    "prebuild:ios:champs-tracker": "APP_ID=champs-tracker npx expo prebuild --platform ios",
    "buildui:ios:champs-tracker": "open ios/ChampsTracker.xcworkspace"
  },
  "dependencies": {
    "@apollo/client": "^3.13.9",
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/stack": "^7.4.8",
    "expo": "~54.0.0",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.12",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.6",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~15.0.7",
    "graphql": "^16.11.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/react-native": "^13.2.2",
    "@types/react": "~19.1.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.11",
    "prettier": "^3.6.2",
    "react-test-renderer": "^19.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
