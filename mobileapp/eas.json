{
  "cli": {
    "version": ">= 9.0.0"
  },
  "build": {
    "placeholder": {
      "env": {
        "APP_ID": "placeholder",
        "EXPO_UPDATES_URL": "https://u.expo.dev/placeholder-branch"
      },
      "distribution": "internal"
    },
    "groceries": {
      "env": { "APP_ID": "groceries", "EXPO_UPDATES_URL": "https://u.expo.dev/groceries-branch" },
      "distribution": "internal"
    },
    "trips": {
      "env": { "APP_ID": "trips", "EXPO_UPDATES_URL": "https://u.expo.dev/trips-branch" },
      "distribution": "internal"
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "placeholder": {},
    "groceries": {},
    "trips": {},
    "production": {}
  },
  "update": {
    "placeholder": { "channel": "family-placeholder" },
    "groceries": { "channel": "family-groceries" },
    "trips": { "channel": "family-trips" }
  }
}
